{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "s3:PutObject",
        "s3:GetObject",
        "s3:DeleteObject",
        "s3:GetObjectAttributes"
      ],
      "Resource": [
        "arn:aws:s3:::testbucket/*"
      ]
    },
    {
      "Action": "s3:ListBucket",
      "Effect": "Allow",
      "Resource": "arn:aws:s3:::testbucket"
    },
    {
      "Action": [
        "admin:CreateServiceAccount",
        "admin:RemoveServiceAccount"
      ],
      "Effect": "Allow"
    }
  ]
}
